<p id="notice"><%= notice %></p>

 
<table class="table table-striped">
    <thead>
    <tr>
      <th>User</th>
      <th>Tarjetum</th>
      <th>Importe</th>
      <th>Puntos</th>
      <th>Premio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <%  Movimiento.where(user_id: user.id) do |movimiento| %>
      <tr>
        <td><%= movimiento.user %></td>
        <td><%= movimiento.tarjetum %></td>
        <td><%= movimiento.importe %></td>
        <td><%= movimiento.puntos %></td>
        <td><%= movimiento.premio %></td>


      </tr>

    <% end %>

  </tbody>
</table>

<br>
