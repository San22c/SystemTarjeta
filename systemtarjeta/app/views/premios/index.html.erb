<p id="notice"><%= notice %></p>

<h1>Listing Premios</h1>

<table>
  <thead>
    <tr>
      <th>Concepto</th>
      <th>Stock</th>
      <th>Puntos</th>
      <th>Fecha baja</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @premios.each do |premio| %>
      <tr>
        <td><%= premio.concepto %></td>
        <td><%= premio.stock %></td>
        <td><%= premio.puntos %></td>
        <td><%= premio.fecha_baja %></td>
        <td><%= link_to 'Show', premio %></td>
        <td><%= link_to 'Edit', edit_premio_path(premio) %></td>
        <td><%= link_to 'Destroy', premio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Premio', new_premio_path %>
